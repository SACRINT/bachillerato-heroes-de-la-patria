# IMPORTANTE: Copia este archivo como .env y cambia los valores

# JWT Secret Key - CAMBIAR POR UNA CLAVE SEGURA ÃšNICA
JWT_SECRET=TU_CLAVE_JWT_SUPER_SEGURA_AQUI_MINIMO_32_CARACTERES

# JWT Expiration Time
JWT_EXPIRES_IN=30m

# Admin Password Hash (se genera automÃ¡ticamente)
ADMIN_PASSWORD_HASH=

# Server Configuration
PORT=3000
NODE_ENV=development

# Session Secret - CAMBIAR POR UNA CLAVE SEGURA ÃšNICA  
SESSION_SECRET=TU_CLAVE_SESSION_SUPER_SEGURA_AQUI_MINIMO_32_CARACTERES

# CORS Origins (URLs permitidas)
CORS_ORIGIN=http://localhost:8080,http://127.0.0.1:8080

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
